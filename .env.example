# Stripe Configuration
# Get these values from your Stripe Dashboard at https://dashboard.stripe.com/apikeys

# Stripe Secret Key (starts with sk_live_ for production or sk_test_ for testing)
STRIPE_SECRET_KEY=sk_live_your_secret_key_here

# Stripe Price ID for your SolarScheduler Pro subscription
STRIPE_PRICE_ID=price_your_price_id_here

# Stripe Webhook Secret (starts with whsec_)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Server Configuration
PORT=3000
NODE_ENV=production